{
  "name": "GeekNote",
  "private": true,
  "dependencies": {
    "@codemirror/lang-markdown": "^6.2.5",
    "@material-symbols/svg-400": "^0.20.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "codemirror": "^6.0.1",
    "esbuild": "^0.12.27",
    "tailwind-material-colors": "^3.0.2",
    "tailwindcss": "^3.4.4"
  },
  "scripts": {
    "build:js": "esbuild app/assets/javascripts/*.* --bundle --format=esm --outdir=app/assets/builds",
    "build:css": "tailwindcss -i app/assets/stylesheets/application.tailwind.css -o app/assets/builds/tailwind.css",
    "build:icon": "cp -r node_modules/@material-symbols vendor/icons"
  },
  "version": "0.1.0"
}
