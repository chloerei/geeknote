{
  "name": "GeekNote",
  "private": true,
  "dependencies": {
    "@chloerei/material-ui": "0.0.11",
    "@codemirror/basic-setup": "^0.18.0",
    "@codemirror/lang-markdown": "^0.18.0",
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.1",
    "@rails/actioncable": "^7.0.1",
    "@rails/activestorage": "^7.0.1",
    "@rails/ujs": "^7.0.1",
    "esbuild": "^0.12.27",
    "local-time": "^2.1.0",
    "material-icons": "^1.10.6",
    "sass": "^1.39.2"
  },
  "scripts": {
    "build:js": "esbuild app/assets/javascripts/*.* --bundle --outdir=app/assets/builds --sourcemap",
    "build:css": "sass ./app/assets/stylesheets/application.scss ./app/assets/builds/application.css --load-path=node_modules --source-map --embed-sources",
    "build:icon": "cp node_modules/material-icons/iconfont/material-icons*.woff* app/assets/builds"
  },
  "version": "0.1.0"
}
