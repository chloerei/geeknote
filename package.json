{
  "name": "GeekNote",
  "private": true,
  "dependencies": {
    "@chloerei/material-ui": "github:chloerei/material-ui",
    "@codemirror/lang-markdown": "^6.2.5",
    "codemirror": "^6.0.1",
    "esbuild": "^0.12.27",
    "material-icons": "^1.10.6",
    "sass": "^1.39.2"
  },
  "scripts": {
    "build:js": "esbuild app/assets/javascripts/*.* --bundle --format=esm --outdir=app/assets/builds",
    "build:css": "sass ./app/assets/stylesheets/application.scss ./app/assets/builds/application.css --load-path=node_modules --source-map --embed-sources",
    "build:icon": "cp node_modules/material-icons/iconfont/material-icons*.woff* app/assets/builds"
  },
  "version": "0.1.0"
}
